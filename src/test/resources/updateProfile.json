{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "token": {
      "type": ["string","null"]
    },
    "model": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "email": {
          "type": "string"
        },
        "role": {
          "type": "string"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "updatedAt": {
          "type": "integer"
        }
      },
      "required": [
        "id",
        "email",
        "role",
        "firstName",
        "lastName",
        "updatedAt"
      ]
    }
  },
  "required": [
    "token",
    "model"
  ]
}